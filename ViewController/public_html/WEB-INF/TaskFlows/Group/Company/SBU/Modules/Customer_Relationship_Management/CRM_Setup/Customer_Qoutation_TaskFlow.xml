<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Customer_Qoutation_TaskFlow">
    <default-activity>ExecuteWithParams</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="Customer_Qoutation">
      <page>/WEB-INF/Fragments/Group/Company/SBU/Modules/Customer_Relationship_Management/CRM_Setup/Customer_Qoutation.jsff</page>
    </view>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>Customer_Qoutation</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
